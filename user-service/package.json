{
  "name": "user-service",
  "version": "1.0.0",
  "main": "dist/app.js",
  "scripts": {
    "build": "tsc -p tsconfig.build.json",
    "prestart": "npm run build",
    "watch": "tsc -w -p tsconfig.build.json",
    "serve": "nodemon dist/app.js",
    "start": "concurrently \"npm run watch\" \"npm run serve\"",
    "test": "jest",
    "sonar": "sonar-scanner"
